<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bjpowernode.crm.workbench.dao.TranHistoryDao">

    <insert id="save">
        insert into tbl_tran_history(
            id,
            stage,
            money,
            expectedDate,
            createTime,
            createBy,
            tranId
        )
        values(
            #{id},
            #{stage},
            #{money},
            #{expectedDate},
            #{createTime},
            #{createBy},
            #{tranId}
        )
    </insert>


    <select id="getTranHistoryListById" resultType="com.bjpowernode.crm.workbench.domain.TranHistory">
        select * from tbl_tran_history where tranId=#{tranId}
    </select>

    <insert id="addTranHistory">
        insert into tbl_tran_history(
            id,
            stage,
            money,
            expectedDate,
            createTime,
            createBy,
            tranId
        )
        values(
            #{id},
            #{stage},
            #{money},
            #{expectedDate},
            #{createTime},
            #{createBy},
            #{tranId}
        )
    </insert>

    <select id="getTranHistoryByid" resultType="com.bjpowernode.crm.workbench.domain.TranHistory">
        select * from tbl_tran_history where tranId=#{id}
    </select>

    <delete id="deleteByid">
        delete  from tbl_tran_history where tranId=#{id}
    </delete>
  	
</mapper>

































